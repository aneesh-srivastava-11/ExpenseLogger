{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.js"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "framework": null,
    "env": {
        "PORT": "5000"
    }
}